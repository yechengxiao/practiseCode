unit Unit1;

interface

uses
  Windows, Messages, SysUtils, Variants, Classes, Graphics, Controls, Forms,
  Dialogs, Menus;

type
  TForm1 = class(TForm)
    MainMenu1: TMainMenu;
    mFile: TMenuItem;
    mStyle: TMenuItem;
    N3: TMenuItem;
    mOpen: TMenuItem;
    mSave: TMenuItem;
    N6: TMenuItem;
    mExit: TMenuItem;
    mAlign: TMenuItem;
    mOver: TMenuItem;
    mHide: TMenuItem;
    N11: TMenuItem;
    mOption: TMenuItem;
    OpenDialog1: TOpenDialog;
    PrintDialog1: TPrintDialog;
    procedure mOpenClick(Sender: TObject);
  private
    { Private declarations }
  public
    { Public declarations }
  end;

var
  Form1: TForm1;

implementation

{$R *.dfm}

procedure TForm1.mOpenClick(Sender: TObject);
begin
  mOpen.Enabled := true;
  mStyle.Enabled := False;
end;

end.

